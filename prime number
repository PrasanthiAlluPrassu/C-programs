#include<stdio.h>
int main()
{
	int arr[5];
	int i;
	printf("enter elements into arry \n");
	for(i=0;i<5;i++)
	scanf("%d",&arr[i]);
	int j;
	int flag;
	printf("the prime number of arry \n");
	for(i=0;i<5;i++)
   {
   	flag=0;
   	for(j=2;j<arr[i];j++)
   	{
   		if(arr[i]%j==0)
   		{
   			flag=1;
   			break;
		   }
	   }
	   if(flag==0)
	   printf("%d",arr[i]);
	   }	

}
