l=list(map(int,input().split()))
d={}
for i in l:
        d[i]=1
    else:
        d[i]=d[i]+1
        m=list(d.values())
        r=min(m)
        s=max(m)
        for i in d:
            if d[i]==r
            a=i
            print("min:",i)
            if d[i]==s:
                b=i
                print("max:",i)
                print("dif:",abs(a-b))
